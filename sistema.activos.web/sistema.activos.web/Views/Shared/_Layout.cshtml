    <div class="row">
        <div class="span12">
            <footer id="footer">
                &copy; 2014 Sistema de Activos Fijos
            </footer>
        </div>
    </div>
    </div>
    @Scripts.Render("~/js")
    @RenderSection("scripts", required: false)
</body>
</html>
